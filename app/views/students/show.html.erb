<div class="page-header list-group">
  <h2><%= @student.name %></h2>
  <%= github_link(@student) %>
  <%= heroku_link(@student) %>
</div>

<% if session[:mentor_id] %>
    <%= link_to "edit", edit_student_path(@student), class: 'btn btn-default' %>
    <%= link_to 'delete', @testimonial, method: :delete, class: 'btn btn-danger',
                data: { confirm: "You sure?" } %>
<% end %>